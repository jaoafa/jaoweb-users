    "@types/to-ico": "^1.1.0",
    "core-js": "^3.18.3",
    "nuxt": "^2.15.7",
    "ress": "^4.0.0",
    "sass": "^1.43.2",
    "to-ico": "^1.1.5",
    "vue-loading-template": "^1.3.2",
    "vuejs-paginate": "^2.1.0",
    "vuex-module-decorators": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
